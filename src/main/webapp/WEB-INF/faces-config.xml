<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
          version="2.0">
<application>
   
</application>
    <application>
        <el-resolver>com.divudi.bean.common.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>personInstitution</managed-bean-name>
        <managed-bean-class>com.divudi.bean.common.PersonInstitutionController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>personInstitutionJpa</managed-bean-name>
        <managed-bean-class>com.divudi.facade.PersonInstitutionFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.divudi.entity.PersonInstitution</converter-for-class>
        <converter-class>com.divudi.bean.common.PersonInstitutionConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>personInstitution_create</from-outcome>
            <to-view-id>/personinstitution/personInstitution/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>personInstitution_list</from-outcome>
            <to-view-id>/personinstitution/personInstitution/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>personInstitution_edit</from-outcome>
            <to-view-id>/personinstitution/personInstitution/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>personInstitution_detail</from-outcome>
            <to-view-id>/personinstitution/personInstitution/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>